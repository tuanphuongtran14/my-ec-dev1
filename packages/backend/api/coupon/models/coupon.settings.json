{
  "kind": "collectionType",
  "collectionName": "coupons",
  "info": {
    "name": "Coupon",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "code": {
      "type": "string",
      "regex": "[0-9A-Z]",
      "required": true,
      "unique": true
    },
    "expiry_date": {
      "type": "datetime",
      "required": true
    },
    "discount_percentage": {
      "type": "decimal",
      "default": 0,
      "required": false,
      "max": 60,
      "min": 0
    },
    "discount_amount": {
      "type": "biginteger",
      "min": "50000"
    }
  }
}
